algoritmo "semnome"
// Função :
// Autor :
// Data : 07/03/2017
// Seção de Declarações 
var

preco, b : real
litro : real


inicio
// Seção de Comandos 

escreval("Digite o preço")
leia(preco)

escreval("Valor que deseja Abastecer")
leia(b)

litro <- (b / preco)

escreval("será reabastecido ",litro :2:2)

fimalgoritmo
